{
  "input": {
    "path": "./public/html",
    "extensions": [".html"]
  },
  "output": {
    "path": "./public_dist/pdf",
    "createDirIfNotExist": true,
    "maintainDirStructure": true
  },
  "options": {
    "format": "A4",
    "margin": {
      "top": "1cm",
      "right": "1cm",
      "bottom": "1cm",
      "left": "1cm"
    },
    "printBackground": true,
    "scale": 1,
    "landscape": false,
    "pageRanges": "",
    "headerTemplate": "",
    "footerTemplate": "",
    "timeout": 30000
  }
} 